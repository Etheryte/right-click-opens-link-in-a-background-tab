{
  "manifest_version": 3,
  "name": "Chrome Right Click",
  "description": "Description",
  "version": "0.1",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "run_at": "document_end"
    }
  ]
}